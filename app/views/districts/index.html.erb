<p id="notice"><%= notice %></p>

<h1>Listing Districts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Ubication</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.name %></td>
        <td><%= district.description %></td>
        <td><%= district.ubication %></td>
        <td><%= link_to 'Show', district %></td>
      </tr>
    <% end %>
  </tbody>
</table>
