<p id="notice"><%= notice %></p>

<h1>Listing Trunk Units</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Turn</th>
      <th>Schedule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trunk_units.each do |trunk_unit| %>
      <tr>
        <td><%= trunk_unit.name %></td>
        <td><%= trunk_unit.category %></td>
        <td><%= trunk_unit.turn %></td>
        <td><%= trunk_unit.schedule %></td>
        <td><%= link_to 'Show', trunk_unit %></td>
      </tr>
    <% end %>
  </tbody>
</table>
